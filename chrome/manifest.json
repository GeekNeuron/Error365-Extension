{
  "manifest_version": 3,
  "name": "Error365",
  "version": "1.5",
  "description": "Identifies and explains browser errors in simple language.",
  "permissions": [
    "webNavigation",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/logo16.png"
    }
  },
  "icons": {
    "48": "assets/icons/logo48.png",
    "128": "assets/icons/logo128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "errors.json"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
